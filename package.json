{
  "name": "es6-react-nodewebkit-clock",
  "version": "0.0.0",
  "description": "NodeWebKit clock",
  "main": "es5.js",
  "author": "jason.dusek@gmail.com",
  "license": "Apache 2",
  "dependencies": {
    "jstimezonedetect": "^1.0.5",
    "moment-timezone": "^0.2.5",
    "react": "^0.12.1"
  },
  "devDependencies": {
    "6to5": "^1.14.17",
    "6to5-browserify": "^1.4.0",
    "browserify": "^7.0.0",
    "nodewebkit": "^0.11.2-1"
  },
  "browserify": {
    "transform": [
      "6to5-browserify"
    ]
  },
  "inject-js-start": "./inject.js",
  "scripts": {
    "browser": "browserify ./app.js > browser.js",
    "prepublish":
      "6to5 --modules -o es5.js index.js && 6to5 --modules -d es5/ lib/",
    "start": "browserify --no-bundle-external app.js > nw.js && nodewebkit"
  }
}
